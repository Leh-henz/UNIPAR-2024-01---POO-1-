\\ Exercicio 4: escreva quantos est√£o entre 0 e 100

import java.util.Scanner;
public class numeros{

public static void main (String[] args){

	Scanner scanner = new Scanner(System.in);
	
int numeros = 0;

for (int n = 1; n <= 20; n++) {
	System.out.println("Informe o numero que deseja: ");
	numeros = scanner.nextInt();


if (numeros >=0 && numeros <=100) {
	System.out.println("Os seguintes numeros sao maiores que 0 e 100: " + numeros);
}else if (numeros>=101 && numeros <=200){
	System.out.println("Os seguintes numeros sao maiores que 101 e 200: " + numeros);
} else { 
System.out.println("Esses numeros sao maiores que 200: " + numeros);
	  }
    
    }
  }	
}
